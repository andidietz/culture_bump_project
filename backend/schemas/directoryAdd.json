{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "headerSituation": "When I arrive late",
            "headerSpecification": "to class",
            "category": "Schools",
            "subcategory": "classroom etiquette",
            "username": "username Ben"
        }
    ],
    "required": [
        "headerSituation",
        "headerSpecification",
        "category",
        "username"
    ],
    "properties": {
        "headerSituation": {
            "$id": "#/properties/headerSituation",
            "type": "string",
            "title": "The headerSituation schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "When I arrive late"
            ]
        },
        "headerSpecification": {
            "$id": "#/properties/headerSpecification",
            "type": "string",
            "title": "The headerSpecification schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "to class"
            ]
        },
        "category": {
            "$id": "#/properties/category",
            "type": "string",
            "title": "The category schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Schools"
            ]
        },
        "subcategory": {
            "$id": "#/properties/subcategory",
            "type": "string",
            "title": "The subcategory schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "classroom etiquette"
            ]
        },
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "username Ben"
            ]
        }
    },
    "additionalProperties": true
}